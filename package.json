{
  "name": "hrms-fontend",
  "version": "1.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "clean": "rm -rf .next yarn.lock node_modules && yarn install",
    "clean:cache": "rm -rf .next && yarn dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write . && eslint . --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "pre-commit": "lint-staged",
    "prepare": "husky || true",
    "check-all": "yarn format:check && yarn lint:strict && yarn type-check && yarn build"
  },
  "dependencies": {
