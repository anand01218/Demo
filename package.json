  "devDependencies": {
    "@types/dropzone": "^5.7.8",
    "@types/hoist-non-react-statics": "^3.3.5",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.8.1",
    "@types/parse-json": "^4.0.2",
    "@types/prop-types": "^15.7.13",
    "@types/react": "^18.3.24",
